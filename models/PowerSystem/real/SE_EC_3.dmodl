	v_2=1
	δ_3=δ_1
	v_3=1
Params:
	w_inj=0.1 //w for inj. measurements
	w_v=0.1	//w for voltage measurements
	δ_1=0		// Parameter name and its value
	aY=32.8797974610715
	aY11=aY
	aY21=aY
	aY22=65.7595949221429
	aY23=aY
	aY32=aY
